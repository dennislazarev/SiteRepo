{
    "name": "bsg/catalogizer",
    "description": "Админка для управления каталогизатором",
    "type": "project",
    "license": "proprietary",
    "require": {
		"vlucas/phpdotenv": "^5.6",
		"firebase/php-jwt": "^6.10",
		"christian-riesen/otp": "^2.0",
		"endroid/qr-code": "^4.8",
		"ramsey/uuid": "^4.9",
        "predis/predis": "^2.0",
        "php": "^8.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-session": "*",
        "ext-ctype": "*",
        "ext-filter": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Src\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "optimize-autoloader": true,
        "classmap-authoritative": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}